
<div class="container">
  <div class="row">
    <div class="<col-md-12></col-md-12>">
      <h2 class="text-center">Datos</h2> <br>
    </div>
  </div>


  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">

      <label>Número</label> <br>
      <input class="form-control" value="<%= @user.number %>" type="text" disabled>

      <label>Nombre</label> <br>
      <input class="form-control" value="<%= @user.name %>" type="text" disabled>

      <label>Email</label> <br>
      <input class="form-control" value="<%= @user.email %>" type="text" disabled>

      <label>Teléfono</label> <br>
      <input class="form-control" value="<%= @user.telefono %>" type="text" disabled>

      <label>Estado</label> <br>
      <input class="form-control" value="<%= @user.state %>" type="text" disabled>

      <br>

      <%= link_to "Volver", root_path %>
      <br><br><br>

    </div>
    <div class="col-md-3"></div>
  </div>

</div>
